{
  "name": "static-site-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "eleventy",
    "bundle": "cd _site && touch .hg && npx parcel build index.html --dist-dir ../_parcel --public-url 'druskus.com' --no-cache",
    "serve": "eleventy --serve",
    "fmt": "prettier --write '{./,./{static,_includes,_data}/**/}*.{json,md,js,css,scss}'",
    "clean": "rm -rf _site .tmp_eleventy .parcel-cache dist _parcel"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@parcel/transformer-sass": "^2.5.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.5.0",
    "parcel-optimizer-friendly-urls": "^0.2.0",
    "posthtml": "^0.16.6",
    "posthtml-urls": "^1.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  }
}
